{
  "name": "nft",
  "license": "MIT",
  "version": "1.0.0",
  "author": "Trader Joe",
  "scripts": {
    "prepare:avax": "mustache config/avax.json nft.template.yaml > nft.avax.yaml",
    "prepare:rinkeby": "mustache config/rinkeby.json nft.template.yaml > nft.rinkeby.yaml",
    "codegen:avax": "graph codegen nft.avax.yaml",
    "codegen:rinkeby": "graph codegen nft.rinkeby.yaml",
    "build:avax": "graph build nft.avax.yaml",
    "build:rinkeby": "graph build nft.rinkeby.yaml",
    "deploy:avax": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ traderjoe-xyz/joe-rocket-subgraphs nft.avax.yaml",
    "deploy:rinkeby": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ traderjoe-xyz/rocket-rinkeby nft.rinkeby.yaml",
    "create-local": "graph create --node http://localhost:8020/ traderjoe-xyz/nft --access-token ",
    "remove-local": "graph remove --node http://localhost:8020/ traderjoe-xyz/nft --access-token ",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 --access-token traderjoe-xyz/joe-rocket-subgraphs nft.fuji.yaml"
  },
  "devDependencies": {
    "@graphprotocol/graph-ts": "^0.24.1",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^2.5.0",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@traderjoe-xyz/rocket-joe": "1.0.0",
    "bignumber.js": "9.0.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eth-saddle": "^0.1.21",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.4",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.9.1",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-storage-layout-diff": "^0.0.3",
    "hardhat-watcher": "^2.1.1",
    "husky": "^7.0.2",
    "jest-diff": "^26.4.2",
    "jest-junit": "^11.1.0",
    "mustache": "^4.2.0",
    "prettier": "^2.4.0",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solium": "^1.2.5",
    "solparse": "^2.2.8"
  }
}
